cat("Number of students failing Chemistry (<=49):", fail_chem, "\n")
cat("Number of students failing Physics (<=49):", fail_phys, "\n")
cat("Highest Chemistry score:", max_chem, "by", paste(students_max_chem, collapse=", "), "\n")
cat("Highest Physics score:", max_phys, "by", paste(students_max_phys, collapse=", "), "\n")
